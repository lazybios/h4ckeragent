        <div class="wrapper clearfix">
            <div class="tips">
                <p>这是个提醒</p>
            </div>
            <div class="left">
                <ul>
                    <li><%= link_to "查看简历", :resumes %></li>
                    <li><%= link_to "创建简历", :new_resume %></li>
                    <li><%= link_to "创建PASSCODE", :new_access %></li>
                    <li><%= link_to "PASSCODE列表", :accesses %></li>
                    <li><a href="">访问统计</a></li>
                </ul>
            </div>
            <div class="right">
                <h2>编辑PASSCODE</h2>
                <div class="right-content">
                    <%= form_for @access do |f| %>
                        <p>PASSCODE备注</p>
                        <p><%= f.text_field :remark %></p>
                        <p>过期时间</p>
                        <p><%= f.text_field :expire_at %></p>
                        <p><%= f.submit "更新" %></p>
                    <% end %>

                </div>
            </div>
        </div>
